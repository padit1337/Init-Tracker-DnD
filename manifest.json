{
  "manifest_version": 2,
  "name": "DnD Initiative Tracker",
  "version": "1.0",
  "description": " A simple extension to track the initiative of DnD-Players. ",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
